{
    "distributed":{
        "random_seed": 42
    },
    "model":{

    },
    "dataset":{
        "data": "cifar",
        "train_batch_size": 32,
        "val_split": 0.2
    },
    "training":{
        "sampling": "iid",
        "total_clients": 50,
        "clients": 5,
        "global_epochs": 5,
        "local_epochs": 10,
        "lr": 0.001,
        "gamma": 0.5,
        "eval_interval": 2000,
        "save_interval": 10000,
        "work_dir": "./trained_models/train-SFPrompt"
    },
    "prompt":{
        "patch": 16,
        "num_token": 5,
        "hidden_size": 768,
        "dropout_rate":0.1,
        "mlp_dom": 3072,
        "num_heads": 12,
        "num_layers": 12,
        "num_classes":10,
        "attention_dropout_rate":0.0,
        "classifier": "token",
        "representation_size": "None"
    },
    "wandb":{
        "logging": false,
        "run_name": "train-SFPrompt"
    }
}